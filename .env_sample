# 圖片來源資料夾路徑
directory=""

# 圖片處理後的輸出資料夾路徑
output_directory=""

# LPIPS聚類輸出目錄
lpips_output_directory=""

# 人臉檢測輸出目錄
face_output_directory=""

# 使用的線程數量，用於多線程處理
num_threads=2

# 啟用圖片完整性驗證（true/false）
enable_validation=true

# 啟用圖片裁切功能（true/false）
enable_cropping=true

# 啟用圖片分類功能（true/false）
enable_classification=true

# 啟用圖片標籤功能（true/false）
enable_tagging=true

# 啟用人臉檢測功能（true/false）
enable_face_detection=true

# 人臉檢測的最小人臉數量，超過此數量的圖片會被處理
min_face_count=2

# 自訂角色標籤，當模型未檢測到角色時使用
custom_character_tag=

# 自訂繪師名稱，若設定則會附加到標籤中
custom_artist_name=

# 啟用wildcard功能（true/false），用於在繪師名稱存在時生成第二行prompts
enable_wildcard=true

# 啟用LPIPS聚類功能（true/false）
enable_lpips_clustering=true

# LPIPS聚類批次處理大小
lpips_batch_size=100

# 啟用圖片放大功能（true/false）
enable_upscaling=true

# 放大後的目標寬度
upscale_target_width=1024

# 放大後的目標高度
upscale_target_height=1024

# 放大使用的模型
upscale_model=HGSR-MHR-anime-aug_X4_320

# 最小尺寸閾值，小於此值的圖片才會被放大，設為空則處理所有圖片
upscale_min_size=800